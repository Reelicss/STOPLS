-- Functions
local function SortEmotes()
	for i, Emote in pairs(Emotes) do
		local EmoteButton = Frame:FindFirstChild(tostring(Emote.id))
		if EmoteButton then
			local IsFavorited = table.find(FavoritedEmotes, Emote.id)
			EmoteButton.LayoutOrder = Emote.sort[CurrentSort] + ((IsFavorited and 0) or #Emotes)
			local numberLabel = EmoteButton:FindFirstChild("number")
			if numberLabel then
				numberLabel.Text = tostring(Emote.sort[CurrentSort])
			end
		end
	end
end

local function createsort(order, text, sort)
	local bt = Instance.new("TextButton")
	bt.Size = UDim2.new(1, -10, 0, 35)
	bt.BackgroundColor3 = Color3.fromRGB(60, 65, 70)
	bt.LayoutOrder = order
	bt.TextColor3 = Color3.new(1, 1, 1)
	bt.Text = text
	bt.Font = Enum.Font.Gotham
	bt.TextSize = 14
	bt.BorderSizePixel = 0
	bt.ZIndex = 11

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0, 4)
	corner.Parent = bt

	bt.MouseEnter:Connect(function()
		bt.BackgroundColor3 = Color3.fromRGB(80, 85, 90)
	end)

	bt.MouseLeave:Connect(function()
		bt.BackgroundColor3 = Color3.fromRGB(60, 65, 70)
	end)

	bt.MouseButton1Click:Connect(function()
		SortFrame.Visible = false
		CurrentSort = sort
		SortEmotes()
	end)

	bt.Parent = SortFrame
	return bt
end

createsort(0, "Custom Order", "customorder")
createsort(1, "Newest First", "newestfirst")
createsort(2, "Oldest First", "oldestfirst")
createsort(3, "Alphabetically A-Z", "alphabeticfirst")
createsort(4, "Alphabetically Z-A", "alphabeticlast")
createsort(5, "Highest Price", "highestprice")
createsort(6, "Lowest Price", "lowestprice")

-- Button events
SortButton.MouseButton1Click:Connect(function()
	SortFrame.Visible = not SortFrame.Visible
end)

CloseButton.MouseButton1Click:Connect(function()
	ScreenGui.Enabled = false
end)

SearchBar:GetPropertyChangedSignal("Text"):Connect(function()
	local txt = SearchBar.Text:lower()
	for _, btn in pairs(Frame:GetChildren()) do
		if btn:IsA("GuiButton") then
			local nm = btn:GetAttribute("name")
			if nm then
				btn.Visible = nm:lower():match(txt) or txt == ""
			end
		end
	end
end)

Frame.MouseLeave:Connect(function()
	EmoteName.Text = "Select an Emote"
end)

-- Hover effects for buttons
local function addButtonHover(button)
	button.MouseEnter:Connect(function()
		TweenService:Create(button, TweenInfo.new(0.1), {
			BackgroundColor3 = Color3.fromRGB(80, 85, 90)
		}):Play()
	end)

	button.MouseLeave:Connect(function()
		TweenService:Create(button, TweenInfo.new(0.1), {
			BackgroundColor3 = Color3.fromRGB(60, 65, 70)
		}):Play()
	end)
end

addButtonHover(SortButton)
addButtonHover(SearchBar)

CloseButton.MouseEnter:Connect(function()
	CloseButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
end)

CloseButton.MouseLeave:Connect(function()
	CloseButton.BackgroundColor3 = Color3.fromRGB(220, 60, 60)
end)

-- Rest of your existing functions (PlayEmote, WaitForChildOfClass, etc.)
local function SendNotification(title, text)
	if (not IsStudio) and syn and syn.toast_notification then
		syn.toast_notification({ Type = ToastType.Error, Title = title, Content = text })
	else
		StarterGui:SetCore("SendNotification", { Title = title, Text = text })
	end
end

local function HumanoidPlayEmote(humanoid, name, id)
	if IsStudio then
		return humanoid:PlayEmote(name)
	else
		return humanoid:PlayEmoteAndGetAnimTrackById(id)
	end
end

local function PlayEmote(name, id)
	SearchBar.Text = ""
	local Humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
	local Description = Humanoid and Humanoid:FindFirstChildOfClass("HumanoidDescription")
	if not (Humanoid and Description) then return end
	if LocalPlayer.Character.Humanoid.RigType ~= Enum.HumanoidRigType.R6 then
		local succ = pcall(function()
			HumanoidPlayEmote(Humanoid, name, id)
		end)
		if not succ then
			Description:AddEmote(name, id)
			HumanoidPlayEmote(Humanoid, name, id)
		end
	else
		SendNotification("R6 Not Supported", "Please use R15 to play emotes")
	end
end

local function WaitForChildOfClass(parent, class)
	local child = parent:FindFirstChildOfClass(class)
	while not child or child.ClassName ~= class do
		child = parent.ChildAdded:Wait()
	end
	return child
end

-- Initialize sorts
for i, v in ipairs(Emotes) do v.sort.customorder = i end

table.sort(Emotes, function(a, b) return a.name:lower() < b.name:lower() end)
for i, v in ipairs(Emotes) do v.sort.alphabeticfirst = i end

table.sort(Emotes, function(a, b) return a.name:lower() > b.name:lower() end)
for i, v in ipairs(Emotes) do v.sort.alphabeticlast = i end

table.sort(Emotes, function(a, b) return a.price < b.price end)
for i, v in ipairs(Emotes) do v.sort.lowestprice = i end

table.sort(Emotes, function(a, b) return a.price > b.price end)
for i, v in ipairs(Emotes) do v.sort.highestprice = i end

table.sort(Emotes, function(a, b) return a.lastupdated > b.lastupdated end)
for i, v in ipairs(Emotes) do v.sort.newestfirst = i end

table.sort(Emotes, function(a, b) return a.lastupdated < b.lastupdated end)
for i, v in ipairs(Emotes) do v.sort.oldestfirst = i end

-- Load favorites
if isfile("FavoritedEmotes.txt") then
	pcall(function()
		FavoritedEmotes = HttpService:JSONDecode(readfile("FavoritedEmotes.txt"))
	end)
else
	writefile("FavoritedEmotes.txt", HttpService:JSONEncode(FavoritedEmotes))
end

-- Character setup
local function CharacterAdded(Character)
	for _, v in pairs(Frame:GetChildren()) do
		if not v:IsA("UIGridLayout") then
			v:Destroy()
		end
	end

	local Humanoid = WaitForChildOfClass(Character, "Humanoid")
	local Description = Humanoid:WaitForChild("HumanoidDescription", 5) or Instance.new("HumanoidDescription", Humanoid)

	-- Random button
	local random = Instance.new("TextButton")
	random.Size = UDim2.new(0, 87, 0, 80)
	random.LayoutOrder = 0
	random.TextColor3 = Color3.new(1, 1, 1)
	random.BorderSizePixel = 0
	random.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	random.BackgroundTransparency = 0.4
	random.Font = Enum.Font.GothamBold
	random.TextSize = 16
	random.Text = "Random"
	random:SetAttribute("name", "Random")

	local randomCorner = Instance.new("UICorner")
	randomCorner.CornerRadius = UDim.new(0, 8)
	randomCorner.Parent = random

	random.MouseButton1Click:Connect(function()
		local randomemote = Emotes[math.random(1, #Emotes)]
		PlayEmote(randomemote.name, randomemote.id)
	end)

	random.MouseEnter:Connect(function()
		EmoteName.Text = "Random Emote"
		random.BackgroundTransparency = 0.2
	end)

	random.MouseLeave:Connect(function()
		random.BackgroundTransparency = 0.4
	end)

	random.Parent = Frame

	-- Create emote buttons
	for i, Emote in pairs(Emotes) do
		Description:AddEmote(Emote.name, Emote.id)

		local EmoteButton = Instance.new("ImageButton")
		local IsFavorited = table.find(FavoritedEmotes, Emote.id)

		EmoteButton.LayoutOrder = Emote.sort[CurrentSort] + ((IsFavorited and 0) or #Emotes)
		EmoteButton.Name = tostring(Emote.id)
		EmoteButton:SetAttribute("name", Emote.name)
		EmoteButton.Size = UDim2.new(0, 87, 0, 80)
		EmoteButton.Image = Emote.icon
		EmoteButton.BackgroundColor3 = Color3.fromRGB(55, 60, 65)
		EmoteButton.BackgroundTransparency = 0.4
		EmoteButton.BorderSizePixel = 0

		local emoteCorner = Instance.new("UICorner")
		emoteCorner.CornerRadius = UDim.new(0, 8)
		emoteCorner.Parent = EmoteButton

		-- Number label
		local EmoteNumber = Instance.new("TextLabel")
		EmoteNumber.Name = "number"
		EmoteNumber.Font = Enum.Font.GothamBold
		EmoteNumber.TextSize = 12
		EmoteNumber.BackgroundTransparency = 1
		EmoteNumber.TextColor3 = Color3.new(1, 1, 1)
		EmoteNumber.BorderSizePixel = 0
		EmoteNumber.Size = UDim2.new(0, 20, 0, 20)
		EmoteNumber.Position = UDim2.new(0, 3, 1, -23)
		EmoteNumber.Text = tostring(Emote.sort[CurrentSort])
		EmoteNumber.TextXAlignment = Enum.TextXAlignment.Center
		EmoteNumber.TextYAlignment = Enum.TextYAlignment.Center

		local UIStroke = Instance.new("UIStroke")
		UIStroke.Thickness = 2
		UIStroke.Color = Color3.fromRGB(0, 0, 0)
		UIStroke.Parent = EmoteNumber

		EmoteNumber.Parent = EmoteButton

		-- Favorite star
		local Favorite = Instance.new("ImageButton")
		Favorite.Name = "favorite"
		Favorite.Image = IsFavorited and FavoriteOn or FavoriteOff
		Favorite.Size = UDim2.new(0, 8, 0, 8)
		Favorite.Position = UDim2.new(1, -15, 1, -15)
		Favorite.BorderSizePixel = 0
		Favorite.BackgroundTransparency = 1
		Favorite.ZIndex = 2
		Favorite.Parent = EmoteButton

		-- Events
		EmoteButton.MouseButton1Click:Connect(function()
			PlayEmote(Emote.name, Emote.id)
		end)

		EmoteButton.MouseEnter:Connect(function()
			EmoteName.Text = Emote.name
			EmoteButton.BackgroundTransparency = 0.2
		end)

		EmoteButton.MouseLeave:Connect(function()
			EmoteButton.BackgroundTransparency = 0.4
		end)

		Favorite.MouseButton1Click:Connect(function()
			local index = table.find(FavoritedEmotes, Emote.id)
			if index then
				table.remove(FavoritedEmotes, index)
				Favorite.Image = FavoriteOff
				EmoteButton.LayoutOrder = Emote.sort[CurrentSort] + #Emotes
			else
				table.insert(FavoritedEmotes, Emote.id)
				Favorite.Image = FavoriteOn
				EmoteButton.LayoutOrder = Emote.sort[CurrentSort]
			end
			writefile("FavoritedEmotes.txt", HttpService:JSONEncode(FavoritedEmotes))
		end)

		EmoteButton.Parent = Frame
	end
end

if LocalPlayer.Character then
	CharacterAdded(LocalPlayer.Character)
end
LocalPlayer.CharacterAdded:Connect(CharacterAdded)

-- Keybind
local function openemotes(_, state)
	if state == Enum.UserInputState.Begin then
		ScreenGui.Enabled = not ScreenGui.Enabled
	end
end

if IsStudio then
	ContextActionService:BindActionAtPriority("Emote Menu", openemotes, true, 2001, Enum.KeyCode.Comma)
else
	ContextActionService:BindCoreActionAtPriority("Emote Menu", openemotes, true, 2001, Enum.KeyCode.Comma)
end

-- Input handling
local inputconnect
ScreenGui:GetPropertyChangedSignal("Enabled"):Connect(function()
	if ScreenGui.Enabled then
		EmoteName.Text = "Select an Emote"
		SearchBar.Text = ""
		SortFrame.Visible = false
		GuiService:SetEmotesMenuOpen(false)
		inputconnect = UserInputService.InputBegan:Connect(function(input, processed)
			if not processed and input.UserInputType == Enum.UserInputType.MouseButton1 then
				local mouse = LocalPlayer:GetMouse()
				local guiObjects = LocalPlayer.PlayerGui:GetGuiObjectsAtPosition(mouse.X, mouse.Y)
				local clickedInGui = false
				for _, obj in ipairs(guiObjects) do
					if obj:IsDescendantOf(ScreenGui) then
						clickedInGui = true
						break
					end
				end
				if not clickedInGui then
					ScreenGui.Enabled = false
				end
			end
		end)
	else
		if inputconnect then inputconnect:Disconnect() end
	end
end)

if not IsStudio then
	GuiService.EmotesMenuOpenChanged:Connect(function(isopen)
		if isopen then
			ScreenGui.Enabled = false
		end
	end)
end

GuiService.MenuOpened:Connect(function()
	ScreenGui.Enabled = false
end)

-- Parent ScreenGui
if not game:IsLoaded() then game.Loaded:Wait() end

if IsStudio then
	ScreenGui.Parent = LocalPlayer.PlayerGui
else
	local SynV3 = syn and DrawingImmediate
	if (not is_sirhurt_closure) and (not SynV3) and (syn and syn.protect_gui) then
		syn.protect_gui(ScreenGui)
		ScreenGui.Parent = CoreGui
	elseif get_hidden_gui or gethui then
		ScreenGui.Parent = (get_hidden_gui or gethui)()
	else
		ScreenGui.Parent = CoreGui
	end
end

print("Improved Emote Wheel loaded! Press ',' (comma) to open.")
